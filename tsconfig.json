{
  "extends": "astro/tsconfigs/strict",
  "baseUrl": ".",
  "paths": {
    "@components/*": ["./src/components/*"],
    "@layouts/*": ["./src/layouts/*"],
    "@interfaces/*": ["./src/interfaces/*"],
    "@sections/*": ["./src/sections/*"],
    "@icons/*": ["./src/icons/*"],
    "@utils/*": ["./src/utils/*"]
  },
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}

